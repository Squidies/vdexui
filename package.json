{
  "name": "rollup-sass-bp",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bs": "node ./config/bs.js",
    "build": "npm-run-all scss postcss rollupjs",
    "build:prod": "npm-run-all scss postcss:prod rollupjs:prod",
    "dev": "npm-run-all build watch:all",
    "postinstallscript": "node ./config/postinstall.js",
    "postinstall": "npm-run-all postinstallscript build",
    "postcss": "node ./config/postcss.js",
    "postcss:prod": "cross-env NODE_ENV=production node ./config/postcss.js",
    "pug": "node ./config/pug.js",
    "pug:prod": "cross-env NODE_ENV=production node ./config/pug.js",
    "rollupjs": "rollup -c ./config/rollup.config.js",
    "rollupjs:prod": "cross-env NODE_ENV=production rollup -c ./config/rollup.config.js",
    "scss": "node ./config/scss.js",
    "watch:all": "npm-run-all -p watch:pug watch:scss watch:js bs",
    "watch:js": "onchange \"./src/**/*.js\" -- npm run rollupjs",
    "watch:pug": "onchange \"./src/pug/**/*.pug\" -- npm run pug",
    "watch:scss": "onchange \"./src/**/*.scss\" -- npm-run-all scss postcss",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "> 1%",
    "IE 11"
  ],
  "dependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.24.5",
    "cross-env": "^5.1.5",
    "css-mqpacker": "^6.0.2",
    "cssnano": "^3.10.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.0.0",
    "postcss": "^6.0.22",
    "pug": "^2.0.3",
    "rollup": "^0.59.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "touch": "^3.1.0"
  }
}
